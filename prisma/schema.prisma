generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Org {
  id          String @id @default(uuid())
  name        String
  author_name String
  email       String @unique
  password    String
  whatsapp    String

  cep          String
  street       String
  neighborhood String
  city         String
  state        String

  latitude  Decimal
  longitude Decimal

  @@map("orgs")
}