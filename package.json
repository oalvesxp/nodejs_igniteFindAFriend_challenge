    "@types/node": "22.10.1",
    "@types/supertest": "6.0.2",
    "eslint": "8.57.1",
    "npm-run-all": "4.1.5",
    "prisma": "6.0.0",
    "supertest": "7.0.0",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "vite-tsconfig-paths": "5.1.3",
    "vitest": "1.0.0"
  },
  "dependencies": {
    "@prisma/client": "6.0.0",
